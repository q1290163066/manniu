/* pages/panel/panel.wxss */
/* 最外侧大盒子 */
page{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.nav{
  margin:20rpx 0;
  height: 200rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background-color: #ffffff;
  padding-top: 50rpx;
}
.inpeople{
  font-size: 25rpx;
}
.whippletree{
  height: 90rpx;
  width: 1rpx;
  border: 1rpx solid #d6d6d6;
  margin-top: 30rpx;
}
.iwpeople{
  text-align: center;
  margin-top: 50rpx;
}
/* 正文内容 */
.content{
  flex: 1;
  overflow: scroll;
  background-color: #ffffff;
}
.role{
  padding: 30rpx 0 0 50rpx;
}
.inwalist{
  padding: 35rpx 0 0 70rpx;
}