.box{
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  color: #424242;
  font-size: 32rpx;
  padding: 0 30rpx;
}
.num{
  color: red;
}
.remark{
  padding: 0 30rpx;
}
.title{
  height: 96rpx;
  line-height: 96rpx;
  font-size: 32rpx;
  color: #424242;
}
.bottom{
  width: 100%;
  height: 86rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: flex-end;
}
.bottom>view{
  margin-right: 30rpx;
  line-height: 74rpx;
}
.btn{
  width: 164rpx;
  height: 74rpx;
  background-color: #ff4344;
  text-align: center;
  color: #fff;
  font-size: 34rpx;
  border-radius: 40rpx;
}
.text{
  font-size: 34rpx;
  color: #666;
}
.allPrice{
  color: #ff4344;
}
.allnum{
  font-size: 28rpx;
  color: #999;
}